<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity2">

    <TextView
        android:id="@+id/vtitulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Clique na foto para ver as informações"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.092" />

    <ImageButton
        android:id="@+id/caoB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/gatoB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vtitulo"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/caov1"
        android:layout_width="59dp"
        android:layout_height="26dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="147dp"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/caov2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/caoB"
        app:layout_constraintTop_toTopOf="@+id/caoB" />

    <TextView
        android:id="@+id/caov2"
        android:layout_width="59dp"
        android:layout_height="26dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="147dp"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/caov3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/caoB"
        app:layout_constraintTop_toBottomOf="@+id/caov1" />

    <TextView
        android:id="@+id/caov3"
        android:layout_width="59dp"
        android:layout_height="26dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="147dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/caoB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/caoB"
        app:layout_constraintTop_toBottomOf="@+id/caov2" />

    <ImageButton
        android:id="@+id/gatoB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/ratoB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/caoB"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/gatoV1"
        android:layout_width="59dp"
        android:layout_height="26dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="147dp"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/gatoV2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/gatoB"
        app:layout_constraintTop_toTopOf="@+id/gatoB" />

    <TextView
        android:id="@+id/gatoV2"
        android:layout_width="59dp"
        android:layout_height="26dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="147dp"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/gatoV3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/gatoB"
        app:layout_constraintTop_toBottomOf="@+id/gatoV1" />


    <TextView
        android:id="@+id/gatoV3"
        android:layout_width="59dp"
        android:layout_height="26dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="147dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/gatoB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/gatoB"
        app:layout_constraintTop_toBottomOf="@+id/gatoV2" />


    <ImageButton
        android:id="@+id/ratoB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gatoB"
        tools:srcCompat="@tools:sample/avatars" />




    <TextView
        android:id="@+id/ratoV1"
        android:layout_width="59dp"
        android:layout_height="26dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="147dp"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/ratoV2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/ratoB"
        app:layout_constraintTop_toTopOf="@+id/ratoB" />

    <TextView
        android:id="@+id/ratoV2"
        android:layout_width="59dp"
        android:layout_height="26dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="147dp"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/ratoV3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/ratoB"
        app:layout_constraintTop_toBottomOf="@+id/ratoV1" />

    <TextView
        android:id="@+id/ratoV3"
        android:layout_width="59dp"
        android:layout_height="26dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="147dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/ratoB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/ratoB"
        app:layout_constraintTop_toBottomOf="@+id/ratoV2" />

</androidx.constraintlayout.widget.ConstraintLayout>